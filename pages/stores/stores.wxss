view.store{
    background-color: white;
    margin: 12px 0;
    height: 68px;
    box-sizing: border-box;
    padding: 8px 16px;

    display: flex;
    align-items: center;
    justify-content: space-between;
}

.store_logo{
    width: 52px;
    height: 52px;
    border-radius: 50%;
    box-shadow: 1px 1px 3px 1px #E0E0E0;
}
.name_id{
    display: flex;
    flex-direction: column;
    justify-content: space-between;

    height: 100%;
}
.name_id .store_name{
    font-size: 16px;
    color: #333;
}
.name_id .store_id{
    font-size: 12px;
    color: #888;
}

.end_at{
    height: 100%;
    font-size: 12px;
    color: #888;
    display: flex;
    align-items: flex-end;
}

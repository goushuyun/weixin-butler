.desp {
  margin: 0 20px;
}
.picker {
  margin: 0 10px;
  box-sizing: border-box;
  height: 32px;
  line-height: 32px;
  border: 1px solid #E0E0E0;
  text-align: left;
  padding: 0 10px;
  border-radius: 10px;
  font-size: 14px;
}
.add_btn {
  margin-top: 10px;
  width: 100%;
  height: 44px;
  line-height: 44px;
  text-align: center;
  background-color: #FFFFFF;
  color: #1581D9;
  font-weight: bold;
}
.item {
  justify-content: space-between;
}
